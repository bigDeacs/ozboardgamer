{
    "name": "vinkla/algolia",
    "description": "An Algolia bridge for Laravel",
    "license": "MIT",
    "keywords": ["laravel", "algolia", "search", "api"],
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "hello@vinkla.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/contracts": "5.0.*|5.1.*",
        "illuminate/support": "5.0.*|5.1.*",
        "graham-campbell/manager": "~2.2",
        "algolia/algoliasearch-client-php": "~1.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "~3.0",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "^4.8|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Vinkla\\Algolia\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "autoload-dev": {
        "psr-4": {
            "Vinkla\\Tests\\Algolia\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
